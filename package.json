{
  "name": "postgretypescript",
  "version": "1.0.0",
  "description": "this is a typescript based server with postgres as a db",
  "main": "./build/main.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": " rm -rf ./build ./dist && tsc -p . && mkdir dist logs  && cp .env dist",
    "dist": "npx javascript-obfuscator build  --output dist/ --compact false --self-defending true",
    "prod": "pm2 start ./ecosystem.config.js --env production",
    "stop": "pm2 stop ./ecosystem.config.js",
    "monitor": "pm2 monit"
  },
  "keywords": [],
  "author": "deformal",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.0",
    "@types/node": "^16.10.3",
    "@types/validator": "^13.6.3",
    "dotenv": "^10.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ip": "^1.1.5",
    "javascript-obfuscator": "^2.19.0",
    "knex": "^0.95.11",
    "nodemon": "^2.0.13",
    "sequelize": "^6.6.5",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3",
    "winston": "^3.3.3",
    "yarn": "^1.22.15"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
