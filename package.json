{
  "name": "postgretypescript",
  "version": "1.0.0",
  "description": "this is a typescript based server with postgres as a db",
  "main": "./build/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": " rm -rf ./build ./dist && tsc -p . && mkdir dist logs  && cp .env dist",
    "dist": "yarn javascript-obfuscator build  --output dist/ --compact false --self-defending true",
    "prod": "pm2 start ./ecosystem.config.js --env production",
    "stop": "pm2 stop ./ecosystem.config.js",
    "monitor": "pm2 monit",
    "migrate": "prisma migrate dev --name public init"
  },
  "keywords": [],
  "author": "drake9211@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@prisma/client": "^3.3.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/ip": "^1.1.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.10.3",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "ip": "^1.1.5",
    "javascript-obfuscator": "^2.19.0",
    "nodemon": "^2.0.13",
    "prisma": "^3.3.0",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3",
    "winston": "^3.3.3",
    "yarn": "^1.22.15"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express-jwt": "^6.0.2",
    "@types/graphql": "^14.5.0",
    "@types/pg": "^8.6.1",
    "apollo-server-core": "^3.4.0",
    "apollo-server-errors": "^3.2.0",
    "apollo-server-express": "^3.4.0",
    "class-validator": "^0.13.1",
